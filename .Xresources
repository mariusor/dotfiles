Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size:  32

! Xft settings ---------------------------------------------------------------

!Xft.dpi:        100
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.autohint:   0


!*faceName:DejaVuSansMono
*faceName:xos4 Terminus
*faceSize:10
*faceStyle:regular
!*font: xft:xos4 Terminus:pixelsize=10
*dynamicColors:      true
*utf8:               2
*eightBitInput:      true
*locale: true
!*saveLines:          512
XTerm*metaSendsEscape: true
XTerm*termName: xterm-256color
XTerm*locale: true
XTerm*utf8:    2

! tangoesque scheme
*background: #111111
*foreground: #babdb6
! Black (not tango) + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMangenta + Mangenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
